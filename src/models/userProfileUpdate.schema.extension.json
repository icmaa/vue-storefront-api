{
  "additionalProperties":true,
  "properties":{
    "customer":{
      "additionalProperties":true,
      "required":[
        "gender",
        "dob"
      ],
      "properties":{
        "addresses":{
          "maxItems":999,
          "items":{
            "properties":{
              "street":{
                "minItems":1,
                "items":{
                  "type":"string",
                  "minLength":0
                }
              },
              "postcode":{
                "type":[
                  "number",
                  "string"
                ]
              },
              "telephone":{
                "type":[
                  "number",
                  "string",
                  "null"
                ]
              },
              "company":{
                "type":[
                  "string",
                  "null"
                ],
                "minLength":0
              },
              "vat_id":{
                "type":[
                  "string",
                  "null"
                ],
                "minLength":3
              }
            }
          }
        },
        "gender":{
          "type":[
            "number",
            "string"
          ],
          "pattern":"[a-zA-Z0-9]+"
        },
        "dob":{
          "type":[
            "null",
            "string"
          ]
        },
        "cluster":{
          "type":[
            "number",
            "string"
          ],
          "pattern":"[a-zA-Z0-9]+"
        },
        "newsletter":{
          "type":"boolean"
        }
      }
    }
  }
}
